{
  "name": "Ticket",
  "plural": "Tickets",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": false
    },
    "email": {
      "type": "string",
      "required": false
    },
    "price": {
      "type": "number",
      "required": false,
      "postgresql": {
        "columnName": "price",
        "dataType": "real"
      }
    },
    "barCode": {
      "type": "string",
      "required": false
    },
    "admittedAt": {
      "type": "date",
      "required": false,
      "default": null
    },
    "collectedAt": {
      "type": "date",
      "required": false,
      "default": null
    }
  },
  "validations": [],
  "relations": {
    "order": {
      "type": "belongsTo",
      "model": "Order"
    },
    "ticketType": {
      "type": "belongsTo",
      "model": "TicketType"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "count"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "nameChange"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "checkTicket"
    }
  ],
  "methods": {}
}
